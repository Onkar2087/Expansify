{
  "version": 2,
  "name": "expanify-backend",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"]
    }
  ],
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "rootDirectory": "backend"
}
